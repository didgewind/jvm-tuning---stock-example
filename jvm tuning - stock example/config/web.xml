<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app PUBLIC '-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN' 'http://java.sun.com/dtd/web-app_2_3.dtd'>

<web-app>
	<display-name>stocks</display-name>
	<description>stocks</description>

	<servlet>
		<servlet-name>stockservlet</servlet-name>
		<display-name>stockservlet</display-name>
		<servlet-class>net.sdo.StockServlet</servlet-class>
	</servlet>


	<servlet-mapping>
		<servlet-name>stockservlet</servlet-name>
		<url-pattern>/stockservlet</url-pattern>
	</servlet-mapping>
	
	<session-config>
		<session-timeout>5</session-timeout>
	</session-config>

	 
</web-app>

